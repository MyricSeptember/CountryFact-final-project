<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.myricseptember.countryfact_start_project">

    <application
            android:name="com.myricseptember.countryfact.CountryFactApp"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.myricseptember.countryfact.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name="com.myricseptember.countryfact.ui.details.CountryDetailsActivity"
                android:label="@string/country_details"
                android:parentActivityName="com.myricseptember.countryfact.ui.MainActivity" />
    </application>

</manifest>